{
  "name": "react-easy-start",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "npm run i18n && react-app-rewired start",
    "build-test": "cross-env PUB_ENV=test node app.js",
    "build-dev": "cross-env PUB_ENV=dev node app.js",
    "build-pre": "cross-env PUB_ENV=pre node app.js",
    "build-prod": "cross-env PUB_ENV=prod node app.js",
    "remove": "node ./node_modules/rimraf/bin.js ./src/packages",
    "build": "yarn submodule && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "mock": "nodemon mock/server.js",
    "i18n": "gulp i18n-json-merge",
    "dev": "react-app-rewired start",
    "submodule": "git submodule update --init --remote",
    "ytt": "node ./node_modules/yapi-to-typescript/lib/cli.js"
  },
