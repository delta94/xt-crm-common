.goods-draggable, .goods-detail-draggable {
  z-index: 999;
  :global(.ant-upload-list-item-actions), :global(.ant-upload-list-item-name) {
    display: none;
  }
  :global(.ant-upload-list-item:hover) {
    .ant-upload-list-item-info {
      background: none
    }
  }
  :global(.ant-upload-list-item-image) {
    width: 86px;
    height: 86px;
  }
}